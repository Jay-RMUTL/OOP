import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("");
        int multiplier = scanner.nextInt();
        
        for (int i = 1; i <= 12; i++) {
            System.out.println(multiplier + " x " + i + " = " + (multiplier * i));
        }
        
        scanner.close();
    }
}
