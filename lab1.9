import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int[] numbers = new int[N];
        double sum = 0;
        
        for (int i = 0; i < N; i++) {
            numbers[i] = sc.nextInt();
            sum += numbers[i];
        }
        
        double average = sum / N;
        System.out.printf("%.2f%n", average);
        
        sc.close();
    }
}
