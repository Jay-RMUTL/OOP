import java.util.Scanner;

public class Main {
    
    // คลาส TempConverter ถูกรวมอยู่ในไฟล์เดียวกัน
    public static class TempConverter {
        
        private TempConverter() {
        }
        
        public static double celsiusToFahrenheit(double celsius) {
            return (celsius * 9.0 / 5.0) + 32;
        }
        
        public static double fahrenheitToCelsius(double fahrenheit) {
            return (fahrenheit - 32) * 5.0 / 9.0;
        }
    }

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        String mode = scanner.nextLine().trim().toUpperCase();
        double temperature = scanner.nextDouble();
        double result = 0.0;

        if (mode.equals("C TO F")) {
            result = TempConverter.celsiusToFahrenheit(temperature);
        } else if (mode.equals("F_TO_C")) {
            result = TempConverter.fahrenheitToCelsius(temperature);
        }
        
        // แสดงผลลัพธ์ที่แปลงค่าแล้ว
        System.out.println(result);
        
        scanner.close();
    }
}
